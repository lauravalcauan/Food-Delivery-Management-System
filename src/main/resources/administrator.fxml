<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="532.0" prefWidth="678.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Presentation.AdministratorController">
   <children>
      <Label layoutX="21.0" layoutY="27.0" text="Products">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <ListView fx:id="listViewProducts" layoutX="21.0" layoutY="56.0" prefHeight="209.0" prefWidth="204.0" />
      <Label layoutX="21.0" layoutY="276.0" text="Details">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <TableView fx:id="tableViewProduct" layoutX="21.0" layoutY="311.0" prefHeight="201.0" prefWidth="440.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Button fx:id="buttonAddToMenu" layoutX="257.0" layoutY="277.0" mnemonicParsing="false" onAction="#handleAddToMenu" text="Add to menu" />
      <ListView fx:id="listViewMenu" layoutX="257.0" layoutY="56.0" prefHeight="209.0" prefWidth="204.0" />
      <Label layoutX="257.0" layoutY="28.0" text="New menu">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button fx:id="buttonCreateMenu" layoutX="373.0" layoutY="277.0" mnemonicParsing="false" onAction="#handleCreateMenu" text="Create menu" />
      <Label layoutX="475.0" layoutY="28.0" text="Manage product">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <TextField fx:id="textFieldTitle" layoutX="475.0" layoutY="57.0" promptText="Title" />
      <TextField fx:id="textFieldRating" layoutX="475.0" layoutY="91.0" promptText="Rating" />
      <TextField fx:id="textFieldCalories" layoutX="475.0" layoutY="124.0" promptText="Calories" />
      <TextField fx:id="textFieldPrice" layoutX="475.0" layoutY="263.0" promptText="Price" />
      <TextField fx:id="textFieldSodium" layoutX="475.0" layoutY="230.0" promptText="Sodium" />
      <TextField fx:id="textFieldFat" layoutX="475.0" layoutY="195.0" promptText="Fat" />
      <TextField fx:id="textFieldProtein" layoutX="475.0" layoutY="161.0" promptText="Protein" />
      <Button fx:id="buttonAddProduct" layoutX="501.0" layoutY="311.0" mnemonicParsing="false" onAction="#handleAddProduct" text="Add product" />
      <Button fx:id="buttonUpdateProduct" layoutX="492.0" layoutY="352.0" mnemonicParsing="false" onAction="#handleUpdateProduct" text="Update product" />
      <Button fx:id="buttonDeleteProduct" layoutX="496.0" layoutY="398.0" mnemonicParsing="false" onAction="#handleDeleteProduct" text="Delete product" />
   </children>
</AnchorPane>
